
  <!--Main layout-->
  <main class="pt-5 mx-lg-5">
    <!-- Page wrapper  -->
    <div class="page-wrapper">
      <!-- Container fluid  -->
      <br>
      <div class="page-breadcrumb bg-white">
        <div class="row align-items-center">
          <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
            <h4 class="page-title text-uppercase font-medium font-14" style="font-weight: bold;">Dashboard/Profile
            </h4>
          </div>
        </div>
        <!-- /.col-lg-12 -->
      </div>
      <div class="container-fluid">
        <div class="row">
          <!-- Column -->
          <div class="col-lg-4 col-xlg-9 col-md-12">
            <div class="card">
              <div class="card-body">
                <form class="form-horizontal form-material">
                  <div class="form-group mb-4">
                    <label class="col-md-12 p-0">Full Name</label>
                    <div class="col-md-12 border-bottom p-0">
                      <span>{{customer.fullName}}</span>
                  </div>
                      </div>
                  <div class="form-group mb-4">
                    <label for="example-email" class="col-md-12 p-0">Email</label>
                    <div class="col-md-12 border-bottom p-0">
                      <span>{{customer.email}}</span>
                   
                    </div>
                  </div>
               
                  <div class="form-group mb-4">
                    <label class="col-md-12 p-0">Age</label>
                    <div class="col-md-12 border-bottom p-0">
                      <span>{{customer.age}}</span>
                    </div>
                  </div>
                  <div class="form-group mb-4">
                    <label class="col-md-12 p-0">health Plan</label>
                    <div class="col-md-12 border-bottom p-0">
                      <span>{{customer.healthPlan}}</span>
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
          <!-- Column -->
        </div>
      </div>
    </div>
  </main>
  <!--Main layout-->
